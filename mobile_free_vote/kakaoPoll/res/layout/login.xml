<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/login_bg"
    android:orientation="vertical" >
    <LinearLayout
        android:id="@+id/linearLayout11"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical" >
        <LinearLayout
            android:id="@+id/linearLayout1"
            android:layout_width="fill_parent"
            android:layout_height="0px"
            android:layout_weight="8"
            android:orientation="vertical" >
        </LinearLayout>
        <LinearLayout
            android:id="@+id/linearLayout2"
            android:layout_width="fill_parent"
            android:layout_height="0px"
            android:layout_weight="14"
            android:gravity="center_horizontal" >
            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="127.3dp"
                android:layout_height="56.28dp"
                android:src="@drawable/logo"/>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/linearLayout3"
            android:layout_width="fill_parent"
            android:layout_height="0px"
            android:layout_weight="34"
            android:gravity="center_horizontal" >
            <ImageView
                android:id="@+id/mal1"
                android:layout_width="123.28dp"
                android:layout_height="114.57dp"
                android:src="@drawable/mal1" />
            <ImageView
                android:id="@+id/mal2"
                android:layout_width="123.28dp"
                android:layout_height="114.57dp"
                android:src="@drawable/mal2" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="0px"
            android:layout_weight="34"
            android:gravity="center_horizontal"
            android:orientation="horizontal" >
            <ImageView
                android:id="@+id/mal3"
                android:layout_width="123.28dp"
                android:layout_height="114.57dp"
                android:src="@drawable/mal3" />
            <ImageView
                android:id="@+id/mal4"
                android:layout_width="123.28dp"
                android:layout_height="114.57dp"
                android:src="@drawable/mal4" />
        </LinearLayout>
    </LinearLayout>
    <com.DooitResearch.kakaoPoll.Bottom_Flipper
        android:id="@+id/bottomflipper"
        android:layout_width="fill_parent"
        android:layout_height="32.83dp"
        android:layout_gravity="bottom" >
    </com.DooitResearch.kakaoPoll.Bottom_Flipper>
    <LinearLayout
        android:id="@+id/loginLayer"
        android:layout_width="250dp"
        android:layout_height="250dp"
        android:layout_gravity="center"
        android:background="@drawable/login_textbox_bg"
        android:gravity="center"
        android:orientation="vertical"
        android:visibility="invisible" >
        <LinearLayout
            android:id="@+id/linearLayout7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >
            <TextView
                android:id="@+id/textView1"
                android:layout_width="35dp"
                android:layout_height="wrap_content"
                android:text=" I D"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:textColor="#ff000000"
                android:textSize="18sp" />
            <EditText
                android:id="@+id/edt1"
                android:layout_width="156dp"
                android:layout_height="wrap_content"
                android:background="@drawable/edit_style"
                android:hint="아이디."
                android:inputType="textVisiblePassword"
                android:nextFocusDown="@+id/edt2"
                android:singleLine="true" >
                <requestFocus />
            </EditText>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/linearLayout8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="3dp"
            android:focusable="true"
            android:focusableInTouchMode="true" >
            <TextView
                android:id="@+id/textView2"
                android:layout_width="35dp"
                android:layout_height="wrap_content"
                android:text="P W"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:textColor="#ff000000"
                android:textSize="18sp" />
            <EditText
                android:id="@+id/edt2"
                android:layout_width="156dp"
                android:layout_height="wrap_content"
                android:background="@drawable/edit_style"
                android:hint="비밀번호."
                android:inputType="textPassword" />
        </LinearLayout>
        <Button
            android:id="@+id/btnin"
            android:layout_width="192.29dp"
            android:layout_height="40.87dp"
            android:layout_marginTop="3dp"
            android:background="@drawable/login_btnin_selector" />
        <LinearLayout
            android:id="@+id/linearLayout9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="3dp"
            android:gravity="center"
            android:orientation="horizontal" >
            <Button
                android:id="@+id/btnJoin"
                android:layout_width="95.14dp"
                android:layout_height="40.2dp"
                android:background="@drawable/login_btnjoin_selector" />
            <Button
                android:id="@+id/btnFound"
                android:layout_width="95.14dp"
                android:layout_height="40.2dp"
                android:background="@drawable/login_btnfound_selector" />
        </LinearLayout>
    </LinearLayout>
</FrameLayout>